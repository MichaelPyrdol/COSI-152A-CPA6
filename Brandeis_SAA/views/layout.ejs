            <a href="/jobs">Jobs</a>
            <a href="/events">Events</a>
            <a href="/contact">Contact</a>
            <a href="/users">Users</a>
        </nav>
        <section>
            <article>
                <div class="flashes">
                    <% if (flashMessages) { %> <% if (flashMessages.success) { %>
                    <div class="flash success"><%= flashMessages.success %></div>
                    <% } else if (flashMessages.error) { %>
                    <div class="flash error"><%= flashMessages.error %></div>
                    <% } %> <% } %>
                </div>
                <%- body%>
            </article>
        </section>
        <br>
        <footer>
            <div id="footerInfo">Â© 2023 Brandeis University</div>
            <div class="social"><a href="#" class="fa fa-facebook"></a></div>
            <div class="social"><a href="#" class="fa fa-linkedin"></a></div>
            <div class="social"><a href="#" class="fa fa-reddit"></a></div>
            <div class="social"><a href="#" class="fa fa-youtube"></a></div>
        </footer>
    </body>
</html>